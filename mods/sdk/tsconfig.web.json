{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "outDir": "./dist/web",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/web/**/*",
    "src/generated/web/**/*",
    "src/web.ts",
    "src/client/WebClient.ts",
    "src/Domains.ts"
  ],
}